    "# This function will print second lowest number\n",
    "def second_lowest_number(n):\n",
    "    n=list(set(n))\n",
    "    n.sort()\n",
    "    return n[1]\n",
    "\n",
    "n=[5,4,6,8,4,8,9,7,897,8,98]\n",
    "print(second_lowest_number(n))"
